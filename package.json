{
  "name": "general-server",
  "version": "1.0.0",
  "description": "A graphql-mongoose-elasticsearch api",
  "main": "index.js",
  "scripts": {
    "file": "node dist/app/utils/test.js",
    "clusters": "ELASTICSEARCH_HOST=127.0.0.1:9200 JWT_SECRET=#test123 NODE_ENV=development node dist/app/clusters",
    "dev": "ELASTICSEARCH_HOST=127.0.0.1:9200 JWT_SECRET=#test123 NODE_ENV=development node_modules/.bin/nodemon --delay 5 dist/app/index.js",
    "build": "npm run gulp build",
    "prod": "ELASTICSEARCH_HOST=127.0.0.1:9200 JWT_SECRET=#test123 NODE_ENV=production npm run gulp",
    "start": "npm run build && npm run clusters",
    "gulp": "node_modules/.bin/gulp",
    "gulp-tests": "ELASTICSEARCH_HOST=127.0.0.1:9200 JWT_SECRET=#test123 NODE_ENV=development node_modules/.bin/gulp --gulpfile src/tests/gulpfile.js",
    "test": "ELASTICSEARCH_HOST=127.0.0.1:9200 JWT_SECRET=#test123 NODE_ENV=development TS_NODE_PROJECT=src/config/tsconfig.json node_modules/.bin/mocha --opts \"src/tests/mocha.opts\" \"src/tests/**/*.test.ts\"",
    "pipelines": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rafael-bruno-ss/server"
  },
  "keywords": [
    "node",
    "graphql",
    "mongodb",
    "elasticsearch"
  ],
  "author": "Rafael Bruno",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.6",
    "@types/chai-http": "^3.0.5",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/elasticsearch": "^5.0.35",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.1",
    "@types/graphql": "^0.13.3",
    "@types/graphql-fields": "^1.3.0",
    "@types/gulp-mocha": "^0.0.32",
    "@types/helmet": "^0.0.42",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.117",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.2.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.14.6",
    "@types/sinon": "^5.0.5",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-mocha": "^6.0.0",
    "gulp-typescript": "^5.0.0-alpha.3",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.3",
    "sinon": "^7.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "elasticsearch": "^16.4.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-compose": "^7.3.0",
    "graphql-compose-elasticsearch": "^4.0.4",
    "graphql-compose-mongoose": "^7.1.4",
    "graphql-fields": "^2.0.1",
    "graphql-tools": "^3.0.5",
    "graphql-type-json": "^0.3.0",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.3.0",
